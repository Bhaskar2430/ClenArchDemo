@model CleanArch.Application.ViewModels.OffersViewModel;
<div class="container body-content">
    <div class="row course-content">
        @foreach(var course in Model.Offers) { 
        <div class="course-image-container ">
            <div class="course-image" style="background-image:url(@course.Id)">
            </div>
        </div>
        <span>@course.Id</span>
        }
    </div>
</div>